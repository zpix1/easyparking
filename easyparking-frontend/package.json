{
  "name": "easyparking-frontend",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "lint": "npx eslint --config .eslintrc.cjs \"src/**/*.{ts,js,svelte}\" --ignore-pattern node_modules/ && npx stylelint --config .stylelintrc.json **/*.{css,scss,svelte} --fix && svelte-check --tsconfig ./tsconfig.json",
    "format": "npx prettier --config .prettierrc --plugin-search-dir=. --write \"src/**/*.{ts,js,svelte}\"",
    "lint:scss:fix": "npx stylelint **/*.{css,scss,svelte} --fix",
    "lint:ts:fix": "npx eslint --config .eslintrc.cjs \"src/**/*.{ts,js,svelte}\" --fix --ignore-pattern node_modules/",
    "preview": "vite preview",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig tsconfig.json --watch"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "@neoconfetti/svelte": "^1.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@tsconfig/svelte": "^2.0.0",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.18",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "sass": "^1.55.0",
    "stylelint": "^14.14.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-scss": "^4.3.0",
    "svelte": "^3.46.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "vite": "^3.1.0"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "svelte-preprocess": "^4.10.7"
  }
}
